    "object-hash": "catalog:sdk",
    "ofetch": "catalog:sdk",
    "socket.io-client": "catalog:sdk"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/object-hash": "^3.0.6",
    "events": "^3.3.0",
    "serve": "^14.2.5",
    "tsdown": "^0.16.5",
    "typedoc": "^0.28.7",
    "typescript": "catalog:",
    "vite": "^7.0.4"
  },
  "engines": {
    "node": "^14 || ^16 || ^18 || ^20 || ^22 || ^24"
