    "./elastic": "./dist/elastic.mjs",
    "./generations": "./dist/generations.mjs",
    "./health": "./dist/health.mjs",
    "./helpers/aggregations": "./dist/helpers/aggregations.mjs",
    "./helpers/figures": "./dist/helpers/figures.mjs",
    "./helpers/filters": "./dist/helpers/filters.mjs",
    "./helpers/generations": "./dist/helpers/generations.mjs",
    "./helpers/layouts": "./dist/helpers/layouts.mjs",
    "./helpers/permissions": "./dist/helpers/permissions.mjs",
    "./helpers/task-presets": "./dist/helpers/task-presets.mjs",
    "./helpers/tasks": "./dist/helpers/tasks.mjs",
    "./helpers/templates": "./dist/helpers/templates.mjs",
