    "preview:docs": "serve docs",
    "type-check": "tsc --noEmit --skipLibCheck",
    "watch:docs": "typedoc --watch"
  },
  "lint-staged": {
    "*.ts": [
      "oxlint --fix"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "dependencies": {
    "date-fns": "catalog:sdk",
