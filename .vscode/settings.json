{
  "files.associations": {
    ".env.defaults": "dotenv",
    ".env": "dotenv"
  },
  "eslint.workingDirectories": [{ "mode": "auto" }],
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "conventionalCommits.scopes": [
    "report",
    "mail",
    "tests",
    "metrics",
    "sdk",
    "vue"
  ],
  "mochaExplorer.cwd": "tests",
  "mochaExplorer.files": "./*/index.spec.ts",
  "mochaExplorer.require": [
    "mocha-steps",
    "ts-node/register"
  ],
  "typescript.tsdk": "./src/services/report/node_modules/typescript/lib",
}